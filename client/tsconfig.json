{
  "compilerOptions": {
    "outDir": "./build",
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": true,
    "jsx": "react"
  },
  "include": [
    "./src/**/*.ts"
, "src/component/test.tsx", "/.d.ts", "src/components/common/sectionheading/SectionHeading.tsx"],
  "exclude": [
    "node_modules",
    "build"
  ]
}
